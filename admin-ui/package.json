    "fuse.js": "6.4.3",
    "install": "^0.13.0",
    "js-beautify": "1.13.0",
    "js-cookie": "2.2.1",
    "jsencrypt": "3.0.0-rc.1",
    "lodash": "^4.17.21",
    "minio": "^8.0.2",
    "moment": "^2.29.1",
    "nprogress": "0.2.0",
    "print-js": "^1.6.0",
    "quill": "1.3.7",
    "screenfull": "5.0.2",
    "swiper": "^5.4.5",
    "uuid": "^8.3.2",
    "vue": "2.6.14",
    "vue-awesome-swiper": "^4.1.1",
