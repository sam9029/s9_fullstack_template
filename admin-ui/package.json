    "babel-eslint": "10.1.0",
    "chalk": "4.1.0",
    "connect": "3.6.6",
    "css": "^3.0.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "7.2.0",
    "lint-staged": "10.5.3",
    "prettier": "^2.7.1",
    "runjs": "4.4.2",
    "sass": "1.32.0",
    "sass-loader": "10.1.0",
    "script-ext-html-webpack-plugin": "2.1.5",
