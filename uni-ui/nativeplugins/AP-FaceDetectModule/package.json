                "AssetsLibrary.framework",
                "CoreTelephony.framework",
                "QuartzCore.framework",
                "CoreFoundation.framework",
                "CoreLocation.framework",
                "ImageIO.framework",
                "CoreMedia.framework",
                "CoreMotion.framework",
                "AVFoundation.framework",
                "WebKit.framework",
                "libresolv.tbd",
                "libz.tbd",
                "libc++.1.tbd",
                "libc++abi.tbd",
                "AudioToolbox.framework",
